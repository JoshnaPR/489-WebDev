<body>
    <div class="container border-bottom">
        <!-- header ( & search bar) -->
        <div class="row">
            <!-- logo -->
            <div class="col-2">
                <img id="logo" src="logo.png">
            </div>
            <!-- search bar -->
            <div class="col">
                <nav class="navbar navbar-light">
                    <form class="form-inline">
                        <img src="location_on_24dp_F59A4C_FILL0_wght400_GRAD0_opsz24.svg" class="img-thumbnail">
